package open.closed.principles.impl;

import open.closed.principles.api.SortType;
import open.closed.principles.Sorter;

public class MergeSort implements Sorter {

	public MergeSort() {
		super.type = SortType.MERGESORT;
	}

	@Override
	public void sort() {
		System.out.println("Sorting with mergesort....");
	}

}
